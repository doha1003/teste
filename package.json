{
  "name": "doha-kr",
  "version": "3.0.0",
  "description": "doha.kr - 심리테스트, 실용도구, AI 운세를 제공하는 웹 플랫폼",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "build": "npm run build:css && npm run build:js && npm run minify:css",
    "build:pwa": "NODE_ENV=production node build-pwa.cjs",
    "build:pwa:dev": "NODE_ENV=development node build-pwa.cjs",
    "build:pwa:watch": "NODE_ENV=development node build-pwa.cjs --watch",
    "build:css": "node tools/build-css.js",
    "build:css:watch": "nodemon --watch css --ext css --exec 'node tools/build-css.js'",
    "build:css:measure": "node tools/measure-css-performance.js",
    "build:js": "node build-js.js",
    "build:js:dev": "node build-js.js --dev",
    "build:js:watch": "node build-js.js --watch --dev",
    "build:js:prod": "NODE_ENV=production rollup -c rollup.config.js",
    "build:js:analyze": "npm run build:js:prod && node analyze-bundle.js",
    "update:html": "node update-html-for-bundles.js",
    "update:html:dev": "node update-html-for-bundles.js --dev",
    "minify:css": "node tools/minify-css.js",
    "optimize:images": "node tools/optimize-images.js",
    "optimize:images:pwa": "node tools/optimize-images-pwa.js",
    "build:sw": "workbox generateSW workbox.config.js",
    "lighthouse:pwa": "lighthouse http://localhost:3000 --only-categories=pwa --output=html --output-path=./lighthouse-pwa-report.html",
    "start": "vercel --prod",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,css,html,md}\"",
    "validate:css": "echo 'CSS validation placeholder'",
    "validate:html": "echo 'HTML validation placeholder'",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "playwright test tests/accessibility",
    "test:security": "playwright test tests/security",
    "test:performance": "playwright test tests/performance",
    "test:korean": "playwright test tests/localization",
    "test:cross-browser": "playwright test tests/cross-browser",
    "test:api": "node test-api-endpoints.js",
    "test:all": "npm run lint && npm run test:coverage && npm run test:e2e && npm run test:accessibility",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:accessibility && npm run test:security && npm run test:performance && npm run test:korean && npm run test:cross-browser",
    "playwright:install": "playwright install --with-deps",
    "playwright:report": "playwright show-report",
    "serve": "python -m http.server 3000",
    "serve:coverage": "python -m http.server 8080 -d coverage",
    "deploy": "vercel --prod",
    "deploy:setup": "node scripts/setup-deployment.js",
    "deploy:check": "node scripts/setup-deployment.js --check-only",
    "deploy:monitor": "node scripts/monitor-deployment.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "cheerio": "^1.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@playwright/test": "^1.54.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.16",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gzip-size-cli": "^5.1.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.0",
    "mini-css-extract-plugin": "^2.9.1",
    "playwright": "^1.54.1",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-custom-properties": "^14.0.1",
    "postcss-import": "^16.1.0",
    "postcss-merge-rules": "^7.0.2",
    "postcss-nested": "^6.2.0",
    "postcss-reporter": "^7.1.0",
    "prettier": "^3.3.3",
    "puppeteer": "^24.15.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.46.2",
    "rollup-plugin-filesize": "^10.0.0",
    "sharp": "^0.33.5",
    "stylelint": "^16.10.0",
    "terser": "^5.27.0",
    "terser-webpack-plugin": "^5.3.10",
    "vitest": "^1.6.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "workbox-cli": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doha1003/teste.git"
  },
  "keywords": [
    "psychology",
    "test",
    "mbti",
    "tools",
    "calculator",
    "fortune",
    "tarot",
    "korean"
  ],
  "author": "doha.kr",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  }
}
